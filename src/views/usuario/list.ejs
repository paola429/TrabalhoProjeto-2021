<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Usuarios</title>
</head>
<body>
    <table>
        <tr>
            <th>id</th>
            <th>nome</th>
            <th>email</th>
            <th>senha</th>
            <th>foto</th>
            <th>edita</th>
            <th>deleta</th>


        </tr>
         <% Usuario.forEach(function(usuario)){
        <tr>
            <td><%=usuario.id%></td>
            <td><%=usuario.nome%></td>
            <td><%=usuario.email%></td>
            <td><%=usuario.senha%></td>
            <td>
                <img src = "/upload/<%=usuario.foto%>" width="80px">
            </td>
            
            <td>edita</td>
            <td>deleta</td>

        
        </tr>

        %> }
    </table>
    
</body>
</html>